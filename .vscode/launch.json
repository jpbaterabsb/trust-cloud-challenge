{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Attach to NestJS",
      "request": "attach",
      "restart": true,
      "protocol": "inspector",
      "address": "localhost",
      "port": 9229,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/app",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}
